<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>databaseconnection</servlet-name>
        <servlet-class>dd.databaseconnection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddVenue</servlet-name>
        <servlet-class>venue.AddVenue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayPhotoServlet</servlet-name>
        <servlet-class>venue.DisplayPhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddImage</servlet-name>
        <servlet-class>dd.AddImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddVenue1</servlet-name>
        <servlet-class>venue.AddVenue1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewImage</servlet-name>
        <servlet-class>dd.ViewImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlannerAcount</servlet-name>
        <servlet-class>dd.PlannerAcount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerAccount</servlet-name>
        <servlet-class>dd.CustomerAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>View</servlet-name>
        <servlet-class>dd.View</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>databaseconnection</servlet-name>
        <url-pattern>/databaseconnection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddVenue</servlet-name>
        <url-pattern>/addvenue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayPhotoServlet</servlet-name>
        <url-pattern>/displayphoto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddImage</servlet-name>
        <url-pattern>/addImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddVenue1</servlet-name>
        <url-pattern>/addvenue1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewImage</servlet-name>
        <url-pattern>/viewImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlannerAcount</servlet-name>
        <url-pattern>/image</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerAccount</servlet-name>
        <url-pattern>/custImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>View</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
